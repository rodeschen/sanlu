<plugin name='hibernate' version='1.3.9' grailsVersion='1.3 &gt; *'>
  <author>Graeme Rocher</author>
  <title>Hibernate for Grails</title>
  <description>A plugin that provides integration between Grails and Hibernate through GORM</description>
  <documentation>http://grails.org/doc/$version</documentation>
  <type>HibernateGrailsPlugin</type>
  <resources>
    <resource>org.grails.Behavior</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.hibernate' name='hibernate-core' version='3.3.1.GA' />
      <dependency group='org.hibernate' name='hibernate-commons-annotations' version='3.1.0.GA' />
      <dependency group='org.hibernate' name='hibernate-annotations' version='3.4.0.GA' />
    </compile>
    <runtime>
      <dependency group='antlr' name='antlr' version='2.7.6' />
      <dependency group='org.hibernate' name='hibernate-ehcache' version='3.3.1.GA' />
      <dependency group='org.hibernate' name='hibernate-validator' version='3.1.0.GA' />
      <dependency group='javassist' name='javassist' version='3.11.0.GA' />
      <dependency group='dom4j' name='dom4j' version='1.6.1' />
    </runtime>
  </dependencies>
  <plugins />
  <runtimePluginRequirements>
    <plugin name='dataSource' version='1.3 &gt; *' />
    <plugin name='i18n' version='1.3 &gt; *' />
    <plugin name='core' version='1.3 &gt; *' />
    <plugin name='domainClass' version='1.3 &gt; *' />
  </runtimePluginRequirements>
  <behavior />
</plugin>